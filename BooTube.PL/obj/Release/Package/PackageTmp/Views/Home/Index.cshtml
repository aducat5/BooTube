@{
    ViewBag.Title = "Anasayfa";
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Anasayfa
        <small>Sizin için önerilenler</small>
    </h1>
</section>
<!-- Main content -->
<section class="content container-fluid">

    @if (Session["user"] != null)
    {

        <div class="box box-danger">
            <div class="box-header"><a class="h3">Takip Ettiklerin <i class="fas fa-chevron-circle-right fa-fw"></i></a></div>
            <div class="box-body">
                @{
                    Html.RenderAction("LastOnesFromFolloweds");
                }
            </div>
        </div>
    }

    <div class="box box-danger">
        <div class="box-header"><a class="h3">Trend Videolar <i class="fas fa-chevron-circle-right fa-fw"></i></a></div>
        <div class="box-body">
            @{
                Html.RenderAction("LastOnesFromTrends");
            }
        </div>
    </div>
</section>